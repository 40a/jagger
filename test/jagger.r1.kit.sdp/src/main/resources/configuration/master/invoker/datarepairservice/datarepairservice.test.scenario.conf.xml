    <bean id="oneCategoryCatalogDataRepairServiceScenario"
          class="com.griddynamics.jagger.invoker.QueryPoolScenarioFactory">
        <property name="loadBalancer">
            <bean class="com.griddynamics.jagger.invoker.RoundRobinLoadBalancer">
                <constructor-arg value="#{oneCategoryCatalogDataRepairServiceQueryProvider.queries()}"/>
                <constructor-arg ref="dataRepairServiceEndpoints"/>
            </bean>
        </property>
        <property name="invokerClazz"
                  value="com.macys.platform.providers.datarepair.test.performance.invoker.ProcessDataRepairRequestCatalogDataRepairServiceInvoker"/>
    </bean>

