    <bean id="searchProductsCatalogServiceScenario" class="com.griddynamics.jagger.invoker.QueryPoolScenarioFactory">
        <property name="loadBalancer">
            <bean class="com.griddynamics.jagger.invoker.RoundRobinLoadBalancer">
                <constructor-arg value="#{searchProductsCatalogServiceQueryProvider.queries()}"/>
                <constructor-arg ref="catalogServiceEndpoints"/>
            </bean>
        </property>
        <property name="invokerClazz" value="com.macys.platform.providers.catalog.test.performance.invoker.SearchProductsCatalogServiceInvoker"/>
    </bean>

