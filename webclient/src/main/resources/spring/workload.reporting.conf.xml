<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="workloadPlotGroups" class="java.util.LinkedHashMap">
        <constructor-arg index="0">
            <map>
                <entry>
                    <key>
                        <bean class="com.griddynamics.jagger.monitoring.reporting.GroupKey">
                            <constructor-arg index="0" value="Throughput"/>
                        </bean>
                    </key>
                    <array value-type="com.griddynamics.jagger.webclient.server.DefaultWorkloadParameters">
                        <value>THROUGHPUT</value>
                    </array>
                </entry>

                <entry>
                    <key>
                        <bean class="com.griddynamics.jagger.monitoring.reporting.GroupKey">
                            <constructor-arg index="0" value="Latency"/>
                        </bean>
                    </key>
                    <array value-type="com.griddynamics.jagger.webclient.server.DefaultWorkloadParameters">
                        <value>LATENCY</value>
                        <value>LATENCY_STD_DEV</value>
                    </array>
                </entry>

                <entry>
                    <key>
                        <bean class="com.griddynamics.jagger.monitoring.reporting.GroupKey">
                            <constructor-arg index="0" value="Time Latency Percentile"/>
                        </bean>
                    </key>
                    <array value-type="com.griddynamics.jagger.webclient.server.DefaultWorkloadParameters">
                        <value>TIME_LATENCY_PERCENTILE_40</value>
                        <value>TIME_LATENCY_PERCENTILE_50</value>
                        <value>TIME_LATENCY_PERCENTILE_60</value>
                        <value>TIME_LATENCY_PERCENTILE_70</value>
                        <value>TIME_LATENCY_PERCENTILE_80</value>
                        <value>TIME_LATENCY_PERCENTILE_90</value>
                        <value>TIME_LATENCY_PERCENTILE_95</value>
                        <value>TIME_LATENCY_PERCENTILE_99</value>
                    </array>
                </entry>

            </map>
        </constructor-arg>
    </bean>

    <bean id="workloadPlotDataProviders" class="java.util.LinkedHashMap">
        <constructor-arg index="0">
            <map>
                <entry>
                    <key>
                        <value type="java.lang.String">Throughput</value>
                    </key>
                    <bean class="com.griddynamics.jagger.webclient.server.plot.ThroughputPlotDataProvider"/>
                </entry>
                <entry>
                    <key>
                        <value type="java.lang.String">Latency</value>
                    </key>
                    <bean class="com.griddynamics.jagger.webclient.server.plot.LatencyPlotDataProvider"/>
                </entry>
                <entry>
                    <key>
                        <value type="java.lang.String">Time Latency Percentile</value>
                    </key>
                    <bean class="com.griddynamics.jagger.webclient.server.plot.TimeLatencyPercentilePlotDataProvider"/>
                </entry>
            </map>
        </constructor-arg>
    </bean>

</beans>
