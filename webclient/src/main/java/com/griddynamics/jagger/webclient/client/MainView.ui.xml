<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with type="com.griddynamics.jagger.webclient.client.mvp.NameTokens" field="tokens" />

    <g:DockLayoutPanel unit="EM" addStyleNames="main-panel">
        <g:north size="2.5">
            <g:HTMLPanel styleName="nav-panel">
                <ul>
                    <li><g:Hyperlink text="Trends" targetHistoryToken="{tokens.trends}"/></li>
                    <li><g:Hyperlink text="View Results" targetHistoryToken="{tokens.viewResults}"/></li>
                </ul>
            </g:HTMLPanel>
        </g:north>
        <g:center>
            <g:DeckLayoutPanel styleName="content-panel" ui:field="contentContainer"/>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>