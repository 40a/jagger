<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>

    <inherits name='com.google.gwt.user.User' />
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.user.theme.standard.Standard"/>

    <entry-point class='com.griddynamics.jagger.webclient.gwt.client.JaggerWebClient'/>
    
    <replace-with class="com.griddynamics.jagger.webclient.gwt.client.ClientFactoryImpl">
        <when-type-is class="com.griddynamics.jagger.webclient.gwt.client.ClientFactory"/>
    </replace-with>

</module>