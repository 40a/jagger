<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:custom="urn:import:com.griddynamics.jagger.webclient.client.components">
    <ui:with type="com.griddynamics.jagger.webclient.client.resources.JaggerResources" field="res"/>
        <g:DockLayoutPanel>
            <g:north size="25">
                <g:HorizontalPanel ui:field="buttonPanel"/>
            </g:north>

            <g:center>
                <g:ScrollPanel addStyleNames="{res.css.overflowYScroll}">
                    <custom:DynamicLayoutPanel ui:field="layoutPanel" />
                </g:ScrollPanel>
            </g:center>
        </g:DockLayoutPanel>
</ui:UiBinder>