<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.google.gwt.user.cellview.client">

    <gwt:SplitLayoutPanel>
        <gwt:center>
            <gwt:HTML>Body</gwt:HTML>
        </gwt:center>
        
        <gwt:south size="200">
            <gwt:SplitLayoutPanel>
                <gwt:west size="500">
                    <gwt:HTMLPanel>
                        <table>
                            <tr>
                                <td>Sessions:</td>
                                <td>
                                    <c:CellTable width="100%" ui:field="sessionTable" styleName="sessionTable" title="Sessions"/>
                                </td>
                            </tr>
                        </table>
                    </gwt:HTMLPanel>
                </gwt:west>
                <gwt:center>
                    <gwt:HTML>CENTER</gwt:HTML>
                </gwt:center>
            </gwt:SplitLayoutPanel>
        </gwt:south>
    </gwt:SplitLayoutPanel>

</ui:UiBinder>